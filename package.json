{
  "name": "expack",
  "version": "1.0.0",
  "description": "Expack is the bare-bones Express and Webpack boilerplate with ES6+ babel transpilation, ESLint linting, Hot Module Reloading, and Jest test framework enabled.",
  "main": "./src/electron.js",
  "scripts": {
    "start": "electron .",
    "webpack": "./node_modules/.bin/webpack-cli --config ./config/webpack.config.js",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder --mac --windows --linux",
    "release": "electron-builder --mac --windows --linux --publish always"
  },
  "keywords": [],
  "build": {
    "appId": "com.yourcompany.yourapp",
    "productName": "Neurodesk Electron App",
    "mac": {
      "category": "public.app-category.lifestyle"
    },
    "dmg": {
      "icon": false
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Office"
    }
  },
  "author": "",
  "license": "CC0-1.0",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "dotenv": "^8.2.0",
    "body-parser": "^1.19.0",
    "node": "^15.4.0",
    "jquery": "^3.5.1",
    "request": "^2.88.2",
    "strip": "^3.0.0"
  },
  "engines": {
    "node": ">=4.3.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "debug": "^4.3.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.2.1",
    "event-stream": "3.3.4",
    "electron": "13.1.2",
    "electron-builder": "20.31.2",
    "gulp": "4.0.0",
    "gulp-concat-css": "3.1.0",
    "gulp-eslint": "5.0.0",
    "gulp-html-replace": "1.6.2",
    "gulp-template": "5.0.0",
    "gulp-watch": "5.0.1",
    "gulp-webserver": "0.9.1",
    "gulp-zip": "4.2.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.4.5",
    "morgan": "^1.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  }
}
